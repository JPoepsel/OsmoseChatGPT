diff --git a/data/app.js b/data/app.js
index 0000000..1111111 100644
--- a/data/app.js
+++ b/data/app.js
@@ -20,6 +20,15 @@ ws.onmessage = e => {
   left.innerText=d.left.toFixed(2);

   addChart(d.tds);
+
+  if(d.cfg){
+    calIn.value  = d.cfg.in;
+    calOut.value = d.cfg.out;
+    prep.value   = d.cfg.prep;
+    flush.value  = d.cfg.flush;
+  }
 };

@@ -44,3 +53,14 @@ window.onload=()=>{
   });
 };
+
+function saveSettings(){
+  ws.send(JSON.stringify({
+    cmd:"save",
+    in:parseInt(calIn.value),
+    out:parseInt(calOut.value),
+    prep:parseInt(prep.value),
+    flush:parseInt(flush.value)
+  }));
+}
diff --git a/src/web.cpp b/src/web.cpp
index 0000000..1111111 100644
--- a/src/web.cpp
+++ b/src/web.cpp
@@ -46,6 +46,17 @@ static void onWsEvent(AsyncWebSocket*, AsyncWebSocketClient*,
   if(m=="start") webRequestStart();
   if(m=="stop")  webRequestStop();
+
+  if(m.startsWith("{")){
+    StaticJsonDocument<128> doc;
+    if(!deserializeJson(doc,m)){
+      if(doc["cmd"]=="save"){
+        WEBDBG("[WEB] settings received\n");
+      }
+    }
+  }
 }
